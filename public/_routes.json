{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/_assets/*",
    "/assets/*",
    "/favicon.ico",
    "/robots.txt",
    "/sitemap.xml",
    "/*.png",
    "/*.jpg",
    "/*.svg",
    "/*.webp",
    "/*.gif",
    "/*.ico"
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
